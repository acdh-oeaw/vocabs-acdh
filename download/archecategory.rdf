<rdf:Description rdf:about="https://vocabs.acdh.oeaw.ac.at/archecategory/3dData">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:definition xml:lang="en">A file containing 3d Data.</skos:definition>
    <skos:definition xml:lang="de">Eine Datei mit 3D-Daten.</skos:definition>
	<skos:inScheme rdf:resource="https://vocabs.acdh.oeaw.ac.at/archecategory/Schema"/>
	<skos:prefLabel xml:lang="de">3D-Daten</skos:prefLabel>
	<skos:prefLabel xml:lang="en">3d data</skos:prefLabel>
	<skos:topConceptOf rdf:resource="https://vocabs.acdh.oeaw.ac.at/archecategory/Schema"/>
</rdf:Description>
<rdf:Description rdf:about="https://vocabs.acdh.oeaw.ac.at/archecategory/audioVisual">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:definition xml:lang="en">A series of visual representations imparting an impression of motion when shown in succession. May or may not include sound. May be used for films, video etc.</skos:definition>
	<skos:definition xml:lang="de">Eine Serie visueller Darstellungen, die Bewegung zeigen, wenn sie hintereinander gezeigt werden. Kann auch Ton enthalten. Kann für Film, Video etc. verwendet werden.</skos:definition>
	<skos:inScheme rdf:resource="https://vocabs.acdh.oeaw.ac.at/archecategory/Schema"/>
	<skos:prefLabel xml:lang="de">Audiovisuelles Medium</skos:prefLabel>
	<skos:prefLabel xml:lang="en">Audio Visual</skos:prefLabel>
	<skos:relatedMatch rdf:resource="http://purl.org/dc/dcmitype/MovingImage"/>
	<skos:topConceptOf rdf:resource="https://vocabs.acdh.oeaw.ac.at/archecategory/Schema"/>
</rdf:Description>
<rdf:Description rdf:about="https://vocabs.acdh.oeaw.ac.at/archecategory/collection">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:definition xml:lang="en">An aggregation of resources of various types, or a list of resources that form part of a publication. If a collection exists of a single type, use the single type to describe it.</skos:definition>
	<skos:definition xml:lang="de">Eine Sammlung von Ressourcen unterschiedlicher Typen oder eine Liste von Ressourcesn, die Teil einer Publikation sind. Wenn die Sammlung aus Ressourcen eines einzigen Typs besteht, sollt der Ressourcentyp verwendet werden.</skos:definition>
	<skos:inScheme rdf:resource="https://vocabs.acdh.oeaw.ac.at/archecategory/Schema"/>
	<skos:prefLabel xml:lang="de">Datensammlung</skos:prefLabel>
	<skos:prefLabel xml:lang="en">Collection</skos:prefLabel>
	<skos:relatedMatch rdf:resource="http://purl.org/dc/dcmitype/Collection"/>
	<skos:topConceptOf rdf:resource="https://vocabs.acdh.oeaw.ac.at/archecategory/Schema"/>
</rdf:Description>
<rdf:Description rdf:about="https://vocabs.acdh.oeaw.ac.at/archecategory/dataset">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:definition xml:lang="en">Data encoded in a defined structure. Could be data from measurements.</skos:definition>
	<skos:definition xml:lang="de">Daten, die einer definierten Strukturierung folgen, wie etwa Daten aus Messreihen.</skos:definition>
	<skos:inScheme rdf:resource="https://vocabs.acdh.oeaw.ac.at/archecategory/Schema"/>
	<skos:prefLabel xml:lang="de">Datenset</skos:prefLabel>
	<skos:prefLabel xml:lang="en">Dataset</skos:prefLabel>
	<skos:relatedMatch rdf:resource="http://purl.org/dc/dcmitype/Dataset"/>
	<skos:topConceptOf rdf:resource="https://vocabs.acdh.oeaw.ac.at/archecategory/Schema"/>
</rdf:Description>
<rdf:Description rdf:about="https://vocabs.acdh.oeaw.ac.at/archecategory/event">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:definition xml:lang="en">A non-persistent, timebased occurrence. Descriptive information and/or content that is the basis for discovery of the purpose, location, duration, and responsible agents associated with an event.</skos:definition>
	<skos:definition xml:lang="de">Eine nicht persistente zeitlich terminierte Begebenheit. Beschreibende Information oder Inhalt, der die Basis zur Auffindug des Grundes, des Ortes, der Dauer und der beteiligten Personen, die mit einem Ereignis assoziiert werden.</skos:definition>
	<skos:inScheme rdf:resource="https://vocabs.acdh.oeaw.ac.at/archecategory/Schema"/>
	<skos:prefLabel xml:lang="de">Ereignis</skos:prefLabel>
	<skos:prefLabel xml:lang="en">Event</skos:prefLabel>
	<skos:relatedMatch rdf:resource="http://purl.org/dc/dcmitype/Event"/>
	<skos:topConceptOf rdf:resource="https://vocabs.acdh.oeaw.ac.at/archecategory/Schema"/>
</rdf:Description>
<rdf:Description rdf:about="https://vocabs.acdh.oeaw.ac.at/archecategory/image">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:definition xml:lang="en">A visual representation other than text. Digitised or born digital images, drawings or photographs.</skos:definition>
	<skos:definition xml:lang="de">Eine visuelle Darstellung, die nicht Text ist. Digitalisierte oder digitale Bilder, Zeichnungen oder Fotografien.</skos:definition>
	<skos:inScheme rdf:resource="https://vocabs.acdh.oeaw.ac.at/archecategory/Schema"/>
	<skos:prefLabel xml:lang="de">Bild</skos:prefLabel>
	<skos:prefLabel xml:lang="en">Image</skos:prefLabel>
	<skos:relatedMatch rdf:resource="http://purl.org/dc/dcmitype/Image"/>
	<skos:topConceptOf rdf:resource="https://vocabs.acdh.oeaw.ac.at/archecategory/Schema"/>
</rdf:Description>
<rdf:Description rdf:about="https://vocabs.acdh.oeaw.ac.at/archecategory/interactiveResource">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:definition xml:lang="en">A resource requiring interaction from the user to be understood, executed, or experienced. Files that require use of a viewer or query/response portals.</skos:definition>
	<skos:definition xml:lang="de">Eine Ressource, die eine Interaktion des Nutzers benötigt, um verstanden, ausgeführt oder erlebt zu werden. Dateien, die einen Viewer oder ein Abfrage/Antwort-Portal benötigen.</skos:definition>
	<skos:inScheme rdf:resource="https://vocabs.acdh.oeaw.ac.at/archecategory/Schema"/>
	<skos:prefLabel xml:lang="de">Interaktive Ressource</skos:prefLabel>
	<skos:prefLabel xml:lang="en">Interactive Resource</skos:prefLabel>
	<skos:relatedMatch rdf:resource="http://purl.org/dc/dcmitype/InteractiveResource"/>
	<skos:topConceptOf rdf:resource="https://vocabs.acdh.oeaw.ac.at/archecategory/Schema"/>
</rdf:Description>
<rdf:Description rdf:about="https://vocabs.acdh.oeaw.ac.at/archecategory/model">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:definition xml:lang="en">An abstract, conceptual, graphical, mathematical or visualisation model that represents empirical objects, phenomena, or physical processes. Modelled descriptions of, for example, different aspects of languages or a molecular biology reaction chain.</skos:definition>
	<skos:definition xml:lang="de">Ein abstraktes, konzeptuelles, grafisches, mathematisches oder visuelles Modell, das empirische Objekte, Phänomene oder physische Prozesse repräsentiert. Modellierte Beschreibungen von beispielsweise verschiedenen Aspekten natürlicher Sprache oder eine molekularbiologische Reaktionskette.</skos:definition>
	<skos:inScheme rdf:resource="https://vocabs.acdh.oeaw.ac.at/archecategory/Schema"/>
	<skos:prefLabel xml:lang="de">Modell</skos:prefLabel>
	<skos:prefLabel xml:lang="en">Model</skos:prefLabel>
	<skos:topConceptOf rdf:resource="https://vocabs.acdh.oeaw.ac.at/archecategory/Schema"/>
</rdf:Description>
<rdf:Description rdf:about="https://vocabs.acdh.oeaw.ac.at/archecategory/other">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:definition xml:lang="en">Choose if none of the other resource tyes applies.</skos:definition>
	<skos:definition xml:lang="de">Wählen, wenn kein anderer Ressourcentyp passt.</skos:definition>
	<skos:inScheme rdf:resource="https://vocabs.acdh.oeaw.ac.at/archecategory/Schema"/>
	<skos:prefLabel xml:lang="de">Andere</skos:prefLabel>
	<skos:prefLabel xml:lang="en">Other</skos:prefLabel>
