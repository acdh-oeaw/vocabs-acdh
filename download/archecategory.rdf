<rdf:Description rdf:about="https://vocabs.acdh.oeaw.ac.at/archecategory/workflow">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:definition xml:lang="en">A structured series of steps which can be executed to produce a final outcome, allowing users a means to specify and enact their work in a more reproducible manner.</skos:definition>
	<skos:definition xml:lang="de">Eine strukturierte Serie von Schritten, die ausgeführt werden können, um ein Ergebnis zu erzeugen und dem Nutzer erlauben Arbeitsschritte zu spezifizieren und zu reproduzieren.</skos:definition>
	<skos:inScheme rdf:resource="https://vocabs.acdh.oeaw.ac.at/archecategory/Schema"/>
	<skos:prefLabel xml:lang="de">Workflow, Arbeitsablauf</skos:prefLabel>
	<skos:prefLabel xml:lang="en">Workflow</skos:prefLabel>
	<skos:topConceptOf rdf:resource="https://vocabs.acdh.oeaw.ac.at/archecategory/Schema"/>
