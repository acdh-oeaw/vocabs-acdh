</rdf:Description>
<rdf:Description rdf:about="https://vocabs.acdh.oeaw.ac.at/archecategory/sound">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:definition xml:lang="en">A resource primarily intended to be heard.  E. g. an audio recording.</skos:definition>
	<skos:definition xml:lang="de">Eine Ressources, die hauptsächlich gehört werden soll, wie etwa eine Tonaufnahme.</skos:definition>
	<skos:inScheme rdf:resource="https://vocabs.acdh.oeaw.ac.at/archecategory/Schema"/>
	<skos:prefLabel xml:lang="de">Audio</skos:prefLabel>
	<skos:prefLabel xml:lang="en">Sound</skos:prefLabel>
	<skos:relatedMatch rdf:resource="http://purl.org/dc/dcmitype/Sound"/>
